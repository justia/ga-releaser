{
  "name": "@justia/releaser",
  "version": "1.2.0",
  "description": "A script to generate releases and changelog",
  "keywords": [
    "justia",
    "release",
    "changelog",
    "internal",
    "tool",
    "github",
    "action"
  ],
  "homepage": "https://github.com/justia/front-end-projects/tree/master/packages/releaser#readme",
  "bugs": {
    "url": "https://github.com/justia/front-end-projects/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justia/front-end-projects.git",
    "directory": "packages/releaser"
  },
  "license": "MIT",
  "author": "Alonso Zuniga <alonso.zuniga@justia.com>",
  "main": "dist/index.js",
  "module": "./dist/index.js",
  "bin": {
    "jReleaser": "./dist/cli/index.js"
  },
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "clean": "tsc -b tsconfig.build.json --clean",
    "postclean": "rimraf dist",
    "prepublishOnly": "safe-publish-latest",
    "rebuild": "npm run clean && npm run build",
    "status": "npm test",
    "poststatus": "npm run typecheck",
    "test": "ts-node --transpile-only ./test/requires.ts",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/exec": "^1.0.4",
    "@actions/github": "^4.0.0",
    "@octokit/plugin-rest-endpoint-methods": "^4.2.1",
    "@release-it/conventional-changelog": "^2.0.0",
    "calver": "^20.5.3",
    "release-it": "^14.2.0",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "@types/tape-async": "^2.3.1",
    "rimraf": "^3.0.2",
    "safe-publish-latest": "^1.1.4",
    "tape-async": "^2.3.0",
    "ts-node": "^8.10.2"
  },
  "engines": {
    "node": ">=10.17",
    "npm": ">=5"
  },
  "publishConfig": {
    "access": "public"
  },
